# Environment Variables for Orchestration System
# Copy this file to .env and fill in your actual values

# Anthropic API Key (get from console.anthropic.com)
# Only needed if using API key auth instead of Pro subscription
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ngrok Auth Token (get from https://dashboard.ngrok.com)
NGROK_AUTHTOKEN=your_ngrok_token_here

# Discord Bot Token (get from https://discord.com/developers/applications)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Discord Webhook URL for responses
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# n8n Configuration (optional overrides)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme
N8N_HOST=localhost
N8N_PORT=5678
