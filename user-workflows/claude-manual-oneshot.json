      "webhookId": "discord-claude",
      "id": "47af93db-9af4-4396-84c1-05d886fb4c01"
    },
    {
      "parameters": {
        "jsCode": "  // Extract message from Discord webhook\n  const content = $input.item.json.body.content || $input.item.json.content || 'No message';\n  const username = $input.item.json.body.username || $input.item.json.username || 'Unknown';\n\n  return [{\n    json: {\n      message: content,\n      user: username,\n      timestamp: new Date().toISOString()\n    }\n  }];"
      },
      "name": "Parse Discord Message",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        208,
        0
      ],
      "id": "aaeb98b1-f552-45ad-aa3b-7aee5f294510"
    },
    {
      "parameters": {
        "jsCode": "const uuid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {\n  const r = Math.random() * 16 | 0;\n  const v = c == 'x' ? r : (r & 0x3 | 0x8);\n  return v.toString(16);\n});\n\nreturn [{\n  json: {\n    sessionId: uuid,\n    message: $input.item.json.message,\n    user: $input.item.json.user\n  }\n}];"
      },
      "name": "Generate Session ID",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        400,
        0
      ],
      "id": "0b4accf1-2881-4c33-8aaa-7c69130378e5"
    },
    {
      "parameters": {
        "command": "=claude -p \"{{ $json.message }}\" --session-id {{ $json.sessionId }} --dangerously-skip-permissions"
